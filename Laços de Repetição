#include <stdio.h>
#include <stdlib.h>
#include <locale.h>
#include <string.h>

int main()
{

    setlocale(LC_ALL, "portuquese");

    int i;
    int i2;
    int i3;

    // i = i + 1 => i++ => i +=1
    // para i de 1 ate 10 passo 1 faca
    

    for (i = 0; i <= 10; i++){

        printf("3 x %d = %d \n", i, 3 * i);
    }

    printf("\n");

    for (i2 = 10; i2 >= 0; i2--){
         printf("5 x %d = %d \n", i2, 5 * i2);
    }

    printf("\n");

    for (i3 = 10; i3 >= -10; i3--){
         printf("7 x %d = %d \n", i3, 7 * i3);
    }

    return 0;
}
